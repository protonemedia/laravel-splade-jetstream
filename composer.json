{
    "name": "protonemedia/laravel-splade-jetstream",
    "description": "Tailwind scaffolding for the Laravel frameworkwith Laravel Splade.",
    "keywords": [
        "laravel",
        "tailwind",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/protonemedia/laravel-splade-jetstream/issues",
        "source": "https://github.com/protonemedia/laravel-splade-jetstream"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/console": "^9.37|^10.0",
        "illuminate/support": "^9.37|^10.0",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.15",
        "protonemedia/laravel-splade": "^1.4.8"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^0.6.5",
        "laravel/pint": "^1.4",
        "laravel/sanctum": "^3.0",
        "livewire/livewire": "^2.12",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Jetstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Jetstream\\Tests\\": "tests/",
            "App\\": "stubs/app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Jetstream\\JetstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}